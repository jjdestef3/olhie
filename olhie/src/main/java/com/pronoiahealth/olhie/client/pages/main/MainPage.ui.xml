<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b='urn:import:com.github.gwtbootstrap.client.ui'
	xmlns:bb='urn:import:com.github.gwtbootstrap.client.ui.base' xmlns:my='urn:import:com.pronoiahealth.olhie.client.widgets'>

	<g:DockLayoutPanel ui:field="dockLayoutPanel" unit="PX">
		<g:north size="42">
			<g:HTMLPanel ui:field="navBarPanel" styleName="header-North-NavContainer">
			</g:HTMLPanel>
		</g:north>
		<g:south size="42">
			<g:HTMLPanel ui:field="footerPlaceHolder" styleName="footer-background">
			</g:HTMLPanel>
		</g:south>
		<g:east size="180">
			<g:HTMLPanel ui:field="newsDisplayPlaceHolder" styleName="east-background">
			</g:HTMLPanel>
		</g:east>
		<g:west size="90">
			<g:HTMLPanel ui:field="sideBarMenuPlaceHolder"
				ui:styleName="section menu">
				<!-- ul class="navigation"> <li class="current"> <a href="index.html"> 
					<i class="icon-book"></i> <span>My Books</span> </a> </li> </ul -->
			</g:HTMLPanel>
		</g:west>
		<g:center>
			<g:HTMLPanel>
				<g:HTMLPanel ui:field="newAssetModalPlaceHolder"></g:HTMLPanel>
				<g:HTMLPanel ui:field="addLogoDialogModalPlaceHolder"></g:HTMLPanel>
				<g:HTMLPanel ui:field="reloadModalPlaceHolder"></g:HTMLPanel>
				<g:HTMLPanel ui:field="navContent" styleName="center-background"
					width="100%" height="100%">
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:center>
	</g:DockLayoutPanel>

</ui:UiBinder>