<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b='urn:import:com.github.gwtbootstrap.client.ui'
	xmlns:bb='urn:import:com.github.gwtbootstrap.client.ui.base'>

	<b:Modal ui:field="newAssetModal" title="What do you want to Add?"
		backdrop="STATIC" keyboard="true" animation="true">

		<b:FluidContainer>
			<b:FluidRow>
				<g:HorizontalPanel styleName="ph-NewBook-NewAssetDialog-AssetTypesPanel">
					<g:FocusPanel ui:field="addFile">
						<g:HTMLPanel styleName="ph-NewBook-NewAssetDialog-Add-Elements">
							<b:Icon type="FILE_ALT" iconSize="LARGE"></b:Icon>
							<g:Label>File</g:Label>
						</g:HTMLPanel>
					</g:FocusPanel>
					<g:FocusPanel ui:field="addLink">
						<g:HTMLPanel styleName="ph-NewBook-NewAssetDialog-Add-Elements">
							<b:Icon type="LINK" iconSize="LARGE"></b:Icon>
							<g:Label>Link</g:Label>
						</g:HTMLPanel>
					</g:FocusPanel>
				</g:HorizontalPanel>
			</b:FluidRow>
		</b:FluidContainer>

	</b:Modal>

</ui:UiBinder>